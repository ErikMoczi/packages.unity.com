  <Removed>
    <View name="TestNode">
      <SelectSet>
        <Select name="GameObjectType" table="RawManagedType">
          <Where column="name" op="equal" value="UnityEngine.GameObject" literal="true"/>
        </Select>
      </SelectSet>
      <!-- columns here are declarations -->
      <Column name="col1"/>
      <Column name="col2"/>
      <Column name="col3" type="ulong"/>
      <Data type="node">
        <!-- When data.type == node -->
        <!--    columns here are declarations -->
        <!-- When data.type == select -->
        <!--    columns here are definitions -->
        <Node name="NodeAllocation">
          <!-- When parent data.type == node -->
          <!--    columns here are entries -->
          <!-- When parent data.type == select -->
          <!--    columns here are declarations-->
          <Column name="col1" value="Native" literal="true"/>
          <Column name="col2" value="Allocation" literal="true"/>

          <Data type="select">
            <SelectSet>
              <Select name="alloc" table="RawNativeAllocation"/>
            </SelectSet>
            <Column name="col1" value="_row_index_" width="42"/>
            <Column name="col2" value="alloc" literal="true"/>
