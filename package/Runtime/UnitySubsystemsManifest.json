{
    "name": "UnityARCore",
    "version": "1.0.0",
    "libraryName": "UnityARCore",
    "cameras": [
    {
        "id": "ARCore-Camera",
        "providesAverageBrightness": true,
        "providesAverageColorTemp": true,
        "providesProjectionMatrix": true,
        "providesDisplayMatrix": true,
        "providesTimestamp": true
    }],
    "inputs": [
    {
        "id": "ARCore-Input"
    }],
    "planes": [
    {
        "id": "ARCore-Plane",
        "supportsPlaneFinding": true
    }],
    "sessions": [
    {
        "id": "ARCore-Session"
    }],
    "referencePoints": [
    {
        "id": "ARCore-ReferencePoint"
    }],
    "depths": [
    {
        "id": "ARCore-Depth"
    }],
    "raycasts": [
    {
        "id": "ARCore-Raycast"
    }]
}
